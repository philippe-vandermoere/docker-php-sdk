{
    "name": "philippe-vandermoere/docker-php-sdk",
    "description": "A Docker PHP client.",
    "type": "library",
    "homepage": "https://github.com/philippe-vandermoere/docker-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe VANDERMOERE",
            "email": "philippe@wizaplace.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2.0",
        "ext-curl": "*",
        "ext-json": "*",
        "php-http/curl-client": "^2.0",
        "guzzlehttp/psr7": "^1.5",
        "zendframework/zend-diactoros": "^2.1",
        "steevanb/php-typed-array": "^2.0",
        "symfony/process": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "PhilippeVandermoere\\DockerPhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Test\\PhilippeVandermoere\\DockerPhpSdk\\": "tests/" }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "phpstan/phpstan": "0.11.8",
        "phpunit/phpunit": "^8.1",
        "fzaninotto/faker": "^1.8"
    }
}
